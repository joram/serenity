server {
  #server_name serenity.oram.ca;
  server_name 192.168.1.7;
  root /var/www;
  index index.html index.htm;
  
  location / {
    try_files $uri $uri/ @root;
  }

  # If nginx can't find a file, fallback to the homepage.
  location @root {
    rewrite .* / redirect;
  }

  {% for s in services %}
    {% if s.name|lower != "home" %}
      {{ s.nginx_location }}
    {% endif %}
  {% endfor %}

  include /etc/nginx/basic.conf;
}

